{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Streamlit Debug",
        "type": "python",
        "request": "launch",
        "program": "${workspaceFolder}/venv/Scripts/streamlit.exe",
        "args": [
          "run",
          "${workspaceFolder}/app/app.py",
          "--logger.level=debug"
        ],
        "console": "integratedTerminal"
      }
    ]
  }
  